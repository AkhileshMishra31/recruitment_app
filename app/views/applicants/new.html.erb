<h1 class="text-center my-4">share your details to get this job</h1>
<div class ="form-box">
  <h1>Register form for Apply</h1>
  <p>Using <a href="https://getbootstrap.com">Bootstrap</a> and <a href="https://www.formbucket.com">FormBucket</a></p>
<%= form_with(model: [@job, @job.applicants.build]) do |form| %>
  <div class="form-group" >
    <%= form.label :name %><br>
    <%= form.text_field :name ,{:class => 'form-control'}%>
  </div>
  <div class="form-group">
    <%= form.label :qualification %><br>
    <%= form.text_field :qualification,{:class => 'form-control'} %>
  </div>
  <div class="form-group">
    <%= form.label :salaryexpected %><br>
    <%= form.text_field :salary,{:class => 'form-control'}%>
  </div>
  <div class="form-group">
    <%= form.label :skills %><br>
    <%= form.text_field :skills,{:class => 'form-control'} %>
  </div>
   <div class="form-group">
    <%= form.label :experience %><br>
    <%= form.text_field :experience,{:class => 'form-control'} %>
  </div>
  <div class="form-group">
    <%= form.label :attachment %>
    <%= form.file_field :attachment,{:class => 'form-control'} %>
  </div> 
  <div>
    <%= form.submit %>
  </div>
<% end %>
</div>
