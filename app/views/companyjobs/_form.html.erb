<%= image_tag @job.avatar if @job.avatar.attached? %>
<%= form_with model: job do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :experience %><br>
    <%= form.text_field :experience %>
  </div>
  <div>
    <%= form.label :salary %><br>
    <%= form.text_field :salary %>
  </div>
  <div>
    <%= form.label :qualification %><br>
    <%= form.text_field :qualification %>
  </div>
   <div>
    <%= form.label :skills %><br>
    <%= form.text_field :skills%> 
  </div>
   <div>
    <%= form.label :"profile-photo" %><br>
    <%= form.file_field :avatar%> 
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
